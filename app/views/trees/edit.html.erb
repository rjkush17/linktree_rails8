<h1>Edit Tree</h1>

<%= form_with model: @tree, local: true do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :facebook %>
    <%= form.text_field :facebook %>
  </div>
  <div>
    <%= form.label :x %>
    <%= form.text_field :x %>
  </div>
  <div>
    <%= form.label :instagram %>
    <%= form.text_field :instagram %>
  </div>
  <div>
    <%= form.label :website %>
    <%= form.text_field :website %>
  </div>
  <div>
    <%= form.submit "Update Tree" %>
  </div>
<% end %>
